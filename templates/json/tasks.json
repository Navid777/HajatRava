{% load filters%}
[
  {% for task in tasks %}
  {
    "id": {{ task.id|android }},
    "title": {{ task.title|android }},
    "done": {{ task.done|android }},
    "assignedTo": {{ task.assigned_to.id|android }},
    "project": {{ task.project.id|android }},
    "creator": {{ task.creator|android }},
    "createDate": {{ task.create_date|date:"d-m-y"|android }},
    "dueDate": {{ task.due_date | date:"d-m-y"|android}}
  }{% if not forloop.last %},{% endif %}
  {% endfor %}
]